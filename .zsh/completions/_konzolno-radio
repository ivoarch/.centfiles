#compdef konzolno-radio

# zsh completions for 'konzolno-radio'

local arguments

arguments=(
    '-h[Показва помощтен текст]'
    '-l[Показва списък на всички радио станции.]'
    '-p[Пусни радио станция от списъка.]'
    '-g[Покажи списък на наличните жанрове. (Комбинирайте със опция -l)]'
    '-s[Въведи URL адреса за стрийминг радио станцията, която желаете.]'
    '-r[Запишете mp3 радио стрийминг на файл.]'
    '-u[Проверява за нова версия, и ако има такава я инсталира]'
    '-w[Аларма]'
)

_arguments -s $arguments
